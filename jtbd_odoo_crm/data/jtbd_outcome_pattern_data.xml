<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1"> <!-- Use noupdate="1" -->

        <!-- Sample Outcome Patterns -->
        <record id="outcome_pattern_client_acquisition" model="jtbd.outcome.pattern">
            <field name="name">Lead Generation Efficiency</field>
            <field name="sequence">10</field>
            <field name="job_category_id" ref="jtbd_job_category_client_acquisition"/>
            <field name="primary_metric">Lead Conversion Rate</field>
            <field name="metric_unit_template">%</field>
            <field name="is_percentage_template">True</field>
            <field name="additional_metrics">Lead Volume
Cost Per Lead (CPL)
Marketing Qualified Leads (MQLs)</field>
            <field name="typical_milestone_1">Implement Lead Tracking System</field>
            <field name="typical_milestone_2">Optimize Top Performing Channels</field>
            <field name="typical_milestone_3">Achieve Target Conversion Rate</field>
            <field name="white_space_suggestions">Develop Referral Program
Implement Account-Based Marketing (ABM)
Expand Content Marketing Efforts</field>
            <field name="notes">Focuses on improving the efficiency and effectiveness of client acquisition efforts.</field>
        </record>

        <record id="outcome_pattern_team_efficiency" model="jtbd.outcome.pattern">
            <field name="name">Operational Throughput</field>
            <field name="sequence">20</field>
            <field name="job_category_id" ref="jtbd_job_category_team_efficiency"/>
            <field name="primary_metric">Billable Utilization Rate</field>
            <field name="metric_unit_template">%</field>
            <field name="is_percentage_template">True</field>
            <field name="additional_metrics">Average Project Turnaround Time (Days)
Administrative Hours per Employee per Week
Project Profit Margin (%)</field>
            <field name="typical_milestone_1">Map Key Delivery Workflows</field>
            <field name="typical_milestone_2">Implement Process Automation Tools/Steps</field>
            <field name="typical_milestone_3">Verify Team Adoption and Efficiency Gains</field>
            <field name="white_space_suggestions">Package Efficient Processes as a Service
Develop Advanced Resource Capacity Planning
Use Freed Capacity for Service Expansion</field>
            <field name="notes">Aimed at agencies looking to optimize internal operations and resource usage.</field>
        </record>

        <record id="outcome_pattern_pricing_strategy" model="jtbd.outcome.pattern">
            <field name="name">Value-Based Pricing Realization</field>
            <field name="sequence">30</field>
            <field name="job_category_id" ref="jtbd_job_category_pricing_strategy"/>
            <field name="primary_metric">Average Revenue Per Client</field>
            <field name="metric_unit_template">$</field>
            <field name="is_percentage_template">False</field>
            <field name="additional_metrics">Proposal Win Rate (%)
Gross Profit Margin (%)
Client Lifetime Value (CLTV)</field>
            <field name="typical_milestone_1">Define New Value-Based Pricing Structure</field>
            <field name="typical_milestone_2">Close First Deal with New Pricing Model</field>
            <field name="typical_milestone_3">Validate Increased Value Perception with Clients</field>
            <field name="white_space_suggestions">Develop Productized Service Offerings
Create Client ROI Measurement Program
Train Team on Value Communication</field>
            <field name="notes">For agencies shifting from cost-plus or hourly billing to value-based models.</field>
        </record>

        <record id="outcome_pattern_client_retention" model="jtbd.outcome.pattern">
            <field name="name">Client Satisfaction &amp; Loyalty</field>
            <field name="sequence">40</field>
            <field name="job_category_id" ref="jtbd_job_category_client_retention"/>
            <field name="primary_metric">Net Promoter Score (NPS)</field>
            <field name="metric_unit_template">Score</field>
            <field name="is_percentage_template">False</field>
            <field name="additional_metrics">Client Churn Rate (%)
Contract Renewal Rate (%)
Client Health Score</field>
            <field name="typical_milestone_1">Implement Client Feedback System</field>
            <field name="typical_milestone_2">Launch Proactive Client Success Program</field>
            <field name="typical_milestone_3">Achieve Target NPS / Renewal Rate</field>
            <field name="white_space_suggestions">Build Client Community Portal
Offer Client Education / Training Programs
Develop Tiered Support Offerings</field>
            <field name="notes">Focuses on improving long-term client relationships and reducing churn.</field>
        </record>

        <record id="outcome_pattern_reporting_systems" model="jtbd.outcome.pattern">
            <field name="name">Client Reporting Automation</field>
            <field name="sequence">50</field>
            <field name="job_category_id" ref="jtbd_job_category_reporting_systems"/>
            <field name="primary_metric">Time Spent on Manual Reporting (Hours/Month)</field>
            <field name="metric_unit_template">Hours</field>
            <field name="is_percentage_template">False</field>
            <field name="additional_metrics">Report Delivery Time (Days)
Data Accuracy Rate (%)
Client Satisfaction with Reports</field>
            <field name="typical_milestone_1">Consolidate Data Sources</field>
            <field name="typical_milestone_2">Implement Automated Reporting Tool</field>
            <field name="typical_milestone_3">Reduce Manual Reporting Time by X%</field>
            <field name="white_space_suggestions">Offer Advanced Analytics Consulting
Develop Custom Client Dashboards
Provide Data Strategy Workshops</field>
            <field name="notes">Addresses the pain point of inefficient and time-consuming client reporting.</field>
        </record>

    </data>
</odoo>